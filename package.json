{
	"name": "pressMusic",
	"displayName": "pressMusic",
	"description": "listen the music by typping code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^0.0.1"
	},
	"keywords": [
		"music"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "pressMusic.hiPressMusic",
				"title": "Hi Press Music"
			}
		],
		"configuration": {
			"title": "Press Music",
			"properties": {
				"pressMusic.enabled": {
					"default": true,
					"type": "boolean",
					"description": "Enable to activate hiPressMusic!!!"
				},
				"pressMusic.musicType": {
					"default": "piano",
					"type": "string",
					"description": "default music type!!!"
				},
				"pressMusic.reminderTime": {
					"default": "1",
					"type": "number",
					"description": "default reminder time interval!"
				},
				"pressMusic.reminderEnabled": {
					"default": true,
					"type": "boolean",
					"description": "Enable to reminder time to rest!"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p -w . --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.61.0",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"@vscode/test-electron": "^1.6.2",
		"copy-webpack-plugin": "^9.0.1",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"ts-loader": "^9.2.5",
		"typescript": "^4.4.3",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0"
	},
	"dependencies": {
		"find-exec": "^1.0.1",
		"play-sound": "^1.1.3"
	}
}
